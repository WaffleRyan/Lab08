<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Layout3"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:id="@+id/Layout3">

    <TextView
        android:id="@+id/t31"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/small_length"
        android:text="AMAZING!!!"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="@id/vertical_guideline"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias=".1"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vertical_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/b31"
        android:layout_width="0dp"
        android:layout_height="@dimen/small_length"
        app:layout_constraintLeft_toLeftOf="@id/vertical_guideline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/t31"
        android:layout_marginTop="@dimen/margin_top"
        android:text="AMAZING" />
    <TextView
        android:id="@+id/t32"
        android:layout_width="@dimen/med_length"
        android:layout_height="@dimen/med_length"
        android:text="AMAZING"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="@id/vertical_guideline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        android:id="@+id/t33"
        android:layout_width="@dimen/med_length"
        android:layout_height="@dimen/med_length"
        android:text="AMAZING"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="@id/vertical_guideline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>